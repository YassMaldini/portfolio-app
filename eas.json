{
  "cli": {
    "version": ">= 2.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "IS_DEV": "true",
        "APP_NAME": "YassBK (dev)",
        "IOS_BUNDLE_ID": "com.ios.dev.yassbk.portfolio",
        "ANDROID_PACKAGE_NAME": "com.android.dev.yassbk.portfolio"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_NAME": "YassBK (preview)",
        "IOS_BUNDLE_ID": "com.ios.preview.yassbk.portfolio",
        "ANDROID_PACKAGE_NAME": "com.android.preview.yassbk.portfolio"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "APP_NAME": "YassBK",
        "IOS_BUNDLE_ID": "com.ios.yassbk.portfolio",
        "ANDROID_PACKAGE_NAME": "com.android.yassbk.portfolio"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "yass.maldini@gmail.com",
        "ascAppId": "1669088544",
        "appleTeamId": "PDZ8AHDZ3L"
      }
    }
  }
}
